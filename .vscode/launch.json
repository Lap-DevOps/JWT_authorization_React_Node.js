{
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": ["run", "dev"],
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/server",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },

    {
      "name": "Client",
      "request": "launch",
      "runtimeArgs": [ "start"],
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/client",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },


  ]
}
